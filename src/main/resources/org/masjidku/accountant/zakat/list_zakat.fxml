<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import java.net.URL?>
<AnchorPane prefHeight="480.0" prefWidth="665.0" styleClass="main" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.masjidku.admin.UserLists">
    <stylesheets>
        <URL value="@../../styles/styles.css" />
        <URL value="@../../styles/fontstyles.css" />
    </stylesheets>
    <children>
        <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="24.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
            <image>
                <Image url="@../../icon/app_icon.png" />
            </image>
        </ImageView>
        <Text fill="#3AAFA9" layoutX="125.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="app-title" text="Aplikasi Desktop MasjidKu " />
        <ImageView fitHeight="32.0" fitWidth="32.0" layoutX="398.0" layoutY="42.0" onMouseClicked="#onLogoutClick" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
            <image>
                <Image url="@../../icon/logout.png" />
            </image>
        </ImageView>

        <Text fill="#3AAFA9" layoutX="14.0" layoutY="172.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="headline" text="KELOLA PENGGUNA">
            <font>
                <Font size="22.0" />
            </font>
        </Text>
        <Button layoutX="30.0" layoutY="191.0" mnemonicParsing="false" onAction="#addUserListener" styleClass="generalButton" text="Tambah Pengguna" />
        <TableView fx:id="userTable" layoutX="30.0" layoutY="224.0" onMouseClicked="#onMouseClicked" prefHeight="200.0" prefWidth="200.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0">
            <columns>
                <TableColumn fx:id="userid" prefWidth="75.0" text="userid" />
                <TableColumn fx:id="username" prefWidth="75.0" text="username" />
                <TableColumn fx:id="jabatan" prefWidth="75.0" text="jabatan" />
                <TableColumn fx:id="status" prefWidth="75.0" text="status" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
      <HBox layoutX="481.0" layoutY="431.0" prefHeight="20.0" spacing="10.0" AnchorPane.bottomAnchor="25.0" AnchorPane.rightAnchor="25.0">
         <children>
            <Button fx:id="btnEdit" disable="true" mnemonicParsing="false" onAction="#editListener" styleClass="generalButton" text="Edit" />
            <Button fx:id="btnReset" disable="true" mnemonicParsing="false" onAction="#onResetListener" styleClass="generalButton" text="Reset" />
            <Button fx:id="btnRemove" disable="true" mnemonicParsing="false" onAction="#onRemoveListener" styleClass="generalButton">
               <graphic>
                  <ImageView cache="true" fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../icon/trash.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
      <Rectangle arcWidth="5.0" fill="#3aafa9" height="5.0" layoutX="101.0" layoutY="-5.0" smooth="false" stroke="BLACK" strokeLineCap="BUTT" strokeLineJoin="BEVEL" strokeMiterLimit="0.0" strokeType="INSIDE" strokeWidth="0.0" width="665.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
    </children>
</AnchorPane>
